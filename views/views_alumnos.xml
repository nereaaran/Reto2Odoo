<odoo>
    <data>
        <record model="ir.ui.view" id="alumnos_tree_view">
            <field name="name">alumnos.tree</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_tree"/>
            <field name="arch" type="xml">
                <field name="login_date" position="after">
                    <field name="status" string="Status"/>
                    <field name="privilege" string="Privilege"/>
                    <field name="tipoUsuario" string="Tipo de usuario"/>
                </field>
            </field>
        </record>
        
        <record model="ir.ui.view" id="alumnos_form_view">
            <field name="name">alumnos.form</field>
            <field name="model">res.users</field>
            <field name="priority" eval="1"/>
            <field name="arch" type="xml">
                <form string="Crear alumno">
                    <sheet>
                        <group>
                            <field name="tipoUsuario"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>